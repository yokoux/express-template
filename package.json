{
  "name": "express-template",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:yokoux/express-template.git",
  "license": "MIT",
  "scripts": {
    "dev": "rollup -c rollup.config.js -w",
    "watch": "nodemon ./lib/index.js",
    "compile": "rollup -c rollup.config.js --configPlugin typescript",
    "clean": "rimraf lib",
    "build": "npm-run-all clean compile",
    "start": "npm-run-all build && node ./lib/index.js",
    "format": "npx prettier --write src",
    "test": "jest"
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "express-fileupload": "^1.2.1",
    "http-status-codes": "^2.2.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-fileupload": "^1.2.0",
    "@types/http-status-codes": "^1.2.0",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.178",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "eslint": "^8.5.0",
    "jest": "^27.4.5",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.62.0",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  }
}
